{
  "name": "flexibile-form",
  "version": "0.9.0",
  "description": "Universal Flex Form",
  "main": "dist/UFForm.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile:FForm": "cross-env NODE_ENV=production webpack",
    "compile:FForm:dev": "cross-env NODE_ENV=development webpack",
    "compile:FForm:profile": "cross-env NODE_ENV=production webpack --profile --json > stats.json",
    "compile:constructor": "cross-env NODE_ENV=production webpack --config ./webpack.constuctor.config.js",
    "compile:constructor:dev": "cross-env NODE_ENV=development webpack --mode=development --config ./webpack.constuctor.config.js",
    "compile:constructor:profile": "cross-env NODE_ENV=production webpack --config ./webpack.constuctor.config.js --profile --json > stats.json",
    "docs": "typedoc --hideGenerator --includeDeclarations --readme none --out src/core/dist --mode modules --tsconfig ./tsconfig.json --excludeExternals --excludePrivate --excludeProtected --theme minimal ./src/core",
    "docs:markdown": "typedoc --hideGenerator --out ./src/core/mdocs --mode modules --tsconfig ./tsconfig.json --excludeExternals --excludePrivate --excludeProtected --theme markdown ./src/core"
  },
