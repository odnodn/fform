{
  "type": "object",
  "properties": {
    "multiselect": {
      "title": "Multiselect",
      "type": "array",
      "ff_data": {
        "fData": {
          "enum": [
            "option 1",
            "option 2",
            "option 3"
          ]
        }
      },
      "ff_managed": true,
      "ff_presets": "multiselect",
      "items": {
        "type": "string",
        "enum": [
          "option 1",
          "option 2",
          "option 3"
        ]
      }
    },
    "array": {
      "type": "array",
      "title": "Array of arrays",
      "default": [
        [
          "string value",
          0,
          false,
          null,
          "more text"
        ]
      ],
      "items": {
        "type": "array",
        "title": "Array of values",
        "ff_presets": "array:noArrayControls",
        "ff_custom": {
          "Title": {
            "style": {
              "width": "100%"
            },
            "children": [
              {},
              {
                "$_ref": "^/parts/ArrayAddButton",
                "children": [
                  "+string"
                ],
                "onClick.bind": [
                  "./",
                  1,
                  {
                    "setOneOf": 0
                  }
                ]
              },
              {
                "$_ref": "^/parts/ArrayAddButton",
                "children": [
                  "+number"
                ],
                "onClick.bind": [
                  "./",
                  1,
                  {
                    "setOneOf": 1
                  }
                ]
              },
              {
                "$_ref": "^/parts/ArrayAddButton",
                "children": [
                  "+boolean"
                ],
                "onClick.bind": [
                  "./",
                  1,
                  {
                    "setOneOf": 2
                  }
                ]
              },
              {
                "$_ref": "^/parts/ArrayDelButton"
              },
              {
                "$_ref": "^/parts/ArrayEmpty"
              },
              {
                "$_ref": "^/parts/ArrayItemMenu",
                "useTag": "span",
                "buttons": [
                  "up",
                  "down",
                  "del"
                ],
                "buttonsProps": {
                  "up": {
                    "children": [
                      "⬆"
                    ]
                  },
                  "down": {
                    "children": [
                      "⬇"
                    ]
                  },
                  "del": {
                    "children": [
                      "x"
                    ]
                  }
                },
                "style": {
                  "float": "right",
                  "marginRight": "1em",
                  "marginLeft": "1em"
                }
              }
            ]
          }
        },
        "maxItems": 7,
        "items": {
          "oneOf": [
            {
              "type": "string",
              "ff_presets": "string:inlineTitle:inlineArrayItem",
              "default": "default"
            },
            {
              "type": [
                "number",
                "null"
              ],
              "ff_presets": "numberNull:inlineTitle:inlineArrayItem",
              "default": null
            },
            {
              "type": "boolean",
              "ff_presets": "boolean:inlineTitle:inlineArrayItem",
              "default": false
            }
          ]
        }
      }
    },
    "booleanLeft": {
      "type": "boolean",
      "title": "boolean left",
      "ff_presets": "booleanLeft",
      "default": false
    },
    "booleanNullLeft": {
      "type": [
        "boolean",
        "null"
      ],
      "title": "booleanNull left",
      "ff_presets": "booleanNullLeft",
      "default": false
    },
    "select": {
      "title": "Select",
      "type": "string",
      "enum": [
        "option 1",
        "option 2",
        "option 3"
      ],
      "ff_presets": "select"
    },
    "textarea": {
      "title": "Textarea",
      "type": "string",
      "ff_presets": "textarea"
    }
  }
}